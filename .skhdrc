# focus window
shift + ctrl - h : yabai -m window --focus west
shift + ctrl - j : yabai -m window --focus south
shift + ctrl - k : yabai -m window --focus north
shift + ctrl - l : yabai -m window --focus east

# move window
shift + ctrl - y : yabai -m window --warp west
shift + ctrl - u : yabai -m window --warp south
shift + ctrl - i : yabai -m window --warp north
shift + ctrl - o : yabai -m window --warp east

# balance size of windows
shift + ctrl - 0 : yabai -m space --balance

# create desktop
shift + ctrl - space : yabai -m space --create

# destroy desktop
shift + ctrl - x : yabai -m space --destroy

# fast focus desktop
# shift + ctrl - p : yabai -m space --focus recent
shift + ctrl - n : yabai -m space --focus prev
shift + ctrl - m : yabai -m space --focus next
shift + ctrl - 1 : yabai -m space --focus 1
shift + ctrl - 2 : yabai -m space --focus 2
shift + ctrl - 3 : yabai -m space --focus 3
shift + ctrl - 4 : yabai -m space --focus 4
shift + ctrl - 5 : yabai -m space --focus 5
shift + ctrl - 6 : yabai -m space --focus 6
shift + ctrl - 7 : yabai -m space --focus 7
shift + ctrl - 8 : yabai -m space --focus 8
shift + ctrl - 9 : yabai -m space --focus 9

# change window size
shift + ctrl - a : yabai -m window --resize left:-20:0 && yabai -m window --resize right:-20:0
shift + ctrl - s : yabai -m window --resize bottom:0:20 && yabai -m window --resize top:0:20
shift + ctrl - w : yabai -m window --resize top:0:-20 && yabai -m window --resize bottom:0:-20
shift + ctrl - d : yabai -m window --resize right:20:0 && yabai -m window --resize left:20:0
